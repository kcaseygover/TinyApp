<!DOCTYPE html>
<html lang="en">
<head>
  <% include partials/_header %>
</head>
<body style="font-size:20px; padding:50px;">
  <% include partials/_header %>
  <% if(user_id) {%>
    <% for (var key in urls) { %>
      <a href="/urls/<%=key%>"><%= key %> </a>
      <%= urls[key]; %>
      <a href= "/urls/<%=key%>">Edit</a>
      <form method="POST" action="/urls/<%=key%>/delete">
        <input type="submit" value="Delete!" />
      </form>
    <% } %>
    <br>
    <br>
    <a href="/urls/new">Shorten a New URL</a>
  <% } %>
</body>
</html>

